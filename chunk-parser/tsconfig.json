{
  "compilerOptions": {
    "target": "ES2023",
    "module": "Preserve",
    "resolveJsonModule": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts"],
}
